{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/next" },
  "defaultProject": "test",
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": { "application": { "style": "css", "linter": "eslint" } }
  },
  "projects": {
    "test": {
      "root": "apps/test",
      "sourceRoot": "apps/test",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": { "root": "apps/test", "outputPath": "dist/apps/test" },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "test:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "test:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "test:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/test"],
          "options": {
            "jestConfig": "apps/test/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/test/**/*.{ts,tsx,js,jsx}"] }
        }
      }
    },
    "test-e2e": {
      "root": "apps/test-e2e",
      "sourceRoot": "apps/test-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/test-e2e/cypress.json",
            "tsConfig": "apps/test-e2e/tsconfig.e2e.json",
            "devServerTarget": "test:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "test:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/test-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "shared-util-session": {
      "root": "libs/shared/util-session",
      "sourceRoot": "libs/shared/util-session/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/util-session/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/util-session"],
          "options": {
            "jestConfig": "libs/shared/util-session/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
